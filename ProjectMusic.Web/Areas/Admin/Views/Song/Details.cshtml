@model ProjectMusic.Entities.Domain.Song

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div>
    <h4>Song Information</h4>
    <hr />

    <div class="mb-2 card card-body w-50">
        <h5 class="card-title">Song Id</h5><h6 class="card-subtitle">@Html.DisplayFor(model => model.SongId)</h6>

        <h5 class="card-title">Song Name</h5><h6 class="card-subtitle">@Html.DisplayFor(model => model.SongName)</h6>

        <div>
            <a href="/Admin/Song/Edit/@Model.SongId" class="btn btn-warning mt-4 main-button">Edit</a>
            <a href="/Admin/Song" class="btn btn-primary mt-4 w-25 float-right">Back to List</a>
        </div>
    </div>
</div>