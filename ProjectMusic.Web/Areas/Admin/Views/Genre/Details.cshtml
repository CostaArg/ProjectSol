@model ProjectMusic.Entities.Domain.Genre

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

    <div>
        <h4>Genre Information</h4>
        <hr />

        <div class="mb-2 card card-body w-50">
            <h5 class="card-title">Album Id</h5><h6 class="card-subtitle">@Html.DisplayFor(model => model.GenreId)</h6>

            <h5 class="card-title">Album Name</h5><h6 class="card-subtitle">@Html.DisplayFor(model => model.GenreName)</h6>

            <h5 class="card-title">Album Price</h5><h6 class="card-subtitle">@Html.DisplayFor(model => model.GenrePhotoUrl)</h6>

            <div>
                <a href="/Admin/Genre/Edit/@Model.GenreId" class="btn btn-warning mt-4 main-button">Edit</a>
                <a href="/Admin/Genre" class="btn btn-primary mt-4 w-25 float-right">Back to List</a>
            </div>
        </div>
    </div>